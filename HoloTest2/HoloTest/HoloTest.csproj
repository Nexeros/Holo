    <PackageReference Include="fo-dicom.Imaging.ImageSharp" Version="5.1.5" />
    <PackageReference Include="HelixToolkit" Version="2.25.0" />
    <PackageReference Include="HelixToolkit.Wpf" Version="2.25.0" />
  </ItemGroup>

</Project>
